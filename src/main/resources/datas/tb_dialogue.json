	30072 : {"id" : 30072, "npc_dialogue" : "（咕噜噜~~~）"},
	30073 : {"id" : 30073, "npc_dialogue" : "沙巴芭提雅阳光热辣辣，香瓜啤酒花风景美如画"},
	30074 : {"id" : 30074, "npc_dialogue" : "（咕~~~）你唱歌就唱歌，能别老说吃的吗"},
	40011 : {"id" : 40011, "npc_dialogue" : "天呐，大海真是太美了"},
	40012 : {"id" : 40012, "npc_dialogue" : "第一次看到海吧"},
	40013 : {"id" : 40013, "npc_dialogue" : "你怎么知道？"},
	40014 : {"id" : 40014, "npc_dialogue" : "不然你激动个啥子"},
	40021 : {"id" : 40021, "npc_dialogue" : "沙滩排球走起"},
	40022 : {"id" : 40022, "npc_dialogue" : "不玩，我要去玩海上降落伞"},
	40023 : {"id" : 40023, "npc_dialogue" : "听起来很刺激啊"},
	40024 : {"id" : 40024, "npc_dialogue" : "会湿身的，当然刺激"},
	40031 : {"id" : 40031, "npc_dialogue" : "你为什么喜欢在海边放风筝啊"},
	40032 : {"id" : 40032, "npc_dialogue" : "因为有风啊"},
	40033 : {"id" : 40033, "npc_dialogue" : "哪里没有风"},
	40034 : {"id" : 40034, "npc_dialogue" : "那种迎着海风，踩着细沙飞奔的感觉，说了你也不懂"},
	40041 : {"id" : 40041, "npc_dialogue" : "小螺号滴滴滴吹，海鸥听了展翅飞"},
	40042 : {"id" : 40042, "npc_dialogue" : "小螺号滴滴滴吹，浪花听了笑微微"},
	40043 : {"id" : 40043, "npc_dialogue" : "茫茫的沙滩，蓝蓝的海水…"},
	40051 : {"id" : 40051, "npc_dialogue" : "我要你陪着我，看那海龟水中游"},
	40052 : {"id" : 40052, "npc_dialogue" : "好啊，什么时候去"},
	40053 : {"id" : 40053, "npc_dialogue" : "慢慢的趴在沙滩上，数着浪花一朵朵"},
	40054 : {"id" : 40054, "npc_dialogue" : "好浪漫"},
	40061 : {"id" : 40061, "npc_dialogue" : "海真的会哭吗？"},
	40062 : {"id" : 40062, "npc_dialogue" : "会，海啸不就是"},
	40063 : {"id" : 40063, "npc_dialogue" : "海啸是由海底地震、火山爆发、海底滑坡等气象变化产生的破坏性海浪……"},
	40064 : {"id" : 40064, "npc_dialogue" : "……学霸，失敬失敬"},
	40071 : {"id" : 40071, "npc_dialogue" : "住海边的人真舒服，可以天天游泳"},
	40072 : {"id" : 40072, "npc_dialogue" : "你住山上会喜欢天天爬山？"},
	40073 : {"id" : 40073, "npc_dialogue" : "额，不会"},
	40074 : {"id" : 40074, "npc_dialogue" : "那不就是，人家天天住海边也不觉得海稀奇啊"},
	50011 : {"id" : 50011, "npc_dialogue" : "游乐场好玩吗？"},
	50012 : {"id" : 50012, "npc_dialogue" : "当然啊，极地快车上下翻滚、螺旋穿梭，极速俯冲"},
	50013 : {"id" : 50013, "npc_dialogue" : "下来之后我还是我吗"},
	50014 : {"id" : 50014, "npc_dialogue" : "那得看你喜不喜欢了"},
	50021 : {"id" : 50021, "npc_dialogue" : "每次看到旋转木马都觉得悲伤"},
	50022 : {"id" : 50022, "npc_dialogue" : "为什么？"},
	50023 : {"id" : 50023, "npc_dialogue" : "音乐停下来就会散场"},
	50024 : {"id" : 50024, "npc_dialogue" : "至少停下来前，你可以短暂的忘记悲伤"},
	50031 : {"id" : 50031, "npc_dialogue" : "你有跟前任去过游乐场吗？"},
	50032 : {"id" : 50032, "npc_dialogue" : "去过，后来就不去了"},
	50033 : {"id" : 50033, "npc_dialogue" : "因为前任？"},
	50034 : {"id" : 50034, "npc_dialogue" : "不是，因为我恐高"},
	50041 : {"id" : 50041, "npc_dialogue" : "真不明白有些人为什么喜欢过山车"},
	50042 : {"id" : 50042, "npc_dialogue" : "那还用说，刺激啊"},
	50043 : {"id" : 50043, "npc_dialogue" : "哪里刺激了，你看那边那个"},
	50044 : {"id" : 50044, "npc_dialogue" : "他不就是因为太刺激才脸色苍白"},
	50051 : {"id" : 50051, "npc_dialogue" : "摩天轮耶"},
	50052 : {"id" : 50052, "npc_dialogue" : "有这么高兴吗"},
	50053 : {"id" : 50053, "npc_dialogue" : "我曾幻想，如果有人在摩天轮的至高点跟我表白，我一定答应"},
	50054 : {"id" : 50054, "npc_dialogue" : "走，我带你去坐"},
	50061 : {"id" : 50061, "npc_dialogue" : "你去过鬼屋吗"},
	50062 : {"id" : 50062, "npc_dialogue" : "不去"},
	50063 : {"id" : 50063, "npc_dialogue" : "嘁，这么大人了还怕鬼"},
	50064 : {"id" : 50064, "npc_dialogue" : "说得好像你老了就不怕死似的"},
	50071 : {"id" : 50071, "npc_dialogue" : "你跟人问路是怎么问的？"},
	50072 : {"id" : 50072, "npc_dialogue" : "某某路怎么走"},
	50073 : {"id" : 50073, "npc_dialogue" : "你先向南走100米，然后向东走200米，再向北走300米，现在你还知道你在哪吗"},
	50074 : {"id" : 50074, "npc_dialogue" : "……为什么不用左右、路口代替东西南北？"},
	60011 : {"id" : 60011, "npc_dialogue" : "你有几个愿望？"},
	60012 : {"id" : 60012, "npc_dialogue" : "我希望世界和平，所有人无病无灾，吃得饱穿得暖……"},
	60013 : {"id" : 60013, "npc_dialogue" : "你自己呢"},
	60014 : {"id" : 60014, "npc_dialogue" : "那……大概是有个人跟我到白头吧"},
	60021 : {"id" : 60021, "npc_dialogue" : "许愿树真可怜，承受这么多人的愿望"},
	60022 : {"id" : 60022, "npc_dialogue" : "更让人心疼的是那些许愿的人"},
	60023 : {"id" : 60023, "npc_dialogue" : "是啊，不知心中的期盼会不会实现，却只能祈祷"},
	60024 : {"id" : 60024, "npc_dialogue" : "或许祈祷就能起作用呢"},
	60031 : {"id" : 60031, "npc_dialogue" : "天灵灵地灵灵，给我分配个对象"},
	60032 : {"id" : 60032, "npc_dialogue" : "天灵地灵你不灵"},
	60033 : {"id" : 60033, "npc_dialogue" : "走开，别打扰我施法"},
	60034 : {"id" : 60034, "npc_dialogue" : "要不你顺便施法也帮我找个对象"},
	60041 : {"id" : 60041, "npc_dialogue" : "我想坐时光机回到从前"},
	60042 : {"id" : 60042, "npc_dialogue" : "穿越回古代？"},
	60043 : {"id" : 60043, "npc_dialogue" : "不是，我的蛋糕被我妈吃了，我要穿越回去先一步吃掉"},
	60044 : {"id" : 60044, "npc_dialogue" : "你真是个……很有想法的吃货"},
	60051 : {"id" : 60051, "npc_dialogue" : "许愿树，我想成为世界第一"},
	60052 : {"id" : 60052, "npc_dialogue" : "哪种第一？"},
	60053 : {"id" : 60053, "npc_dialogue" : "第一聪明，第一有钱，第一学霸，第一……"},
	60054 : {"id" : 60054, "npc_dialogue" : "许愿树说：让你有多远走多远"},
	60061 : {"id" : 60061, "npc_dialogue" : "世纪跟光年有什么区别？"},
	60062 : {"id" : 60062, "npc_dialogue" : "这是许愿树，不是知识库"},
	60063 : {"id" : 60063, "npc_dialogue" : "那你知道吗？"},
	60064 : {"id" : 60064, "npc_dialogue" : "当我没说"},
	60071 : {"id" : 60071, "npc_dialogue" : "爱情为什么这么艰难？"},
	60072 : {"id" : 60072, "npc_dialogue" : "因为人是一种很复杂的情感动物"},
	60073 : {"id" : 60073, "npc_dialogue" : "比如呢？"},
	60074 : {"id" : 60074, "npc_dialogue" : "比如人说一辈子，却是保持眼前所有要素的一辈子，而不是你变成什么样子都是一辈子"},
	70011 : {"id" : 70011, "npc_dialogue" : "女人是不是都喜欢逛街？"},
	70012 : {"id" : 70012, "npc_dialogue" : "这我哪知道"},
	70013 : {"id" : 70013, "npc_dialogue" : "你干嘛来的？"},
	70014 : {"id" : 70014, "npc_dialogue" : "唉，陪朋友来逛商场"},
	70021 : {"id" : 70021, "npc_dialogue" : "今天商场打折"},
	70022 : {"id" : 70022, "npc_dialogue" : "哪家店啊"},
	70023 : {"id" : 70023, "npc_dialogue" : "全部都在做活动啊"},
	70024 : {"id" : 70024, "npc_dialogue" : "择日不如撞日，血拼吧"},
	70031 : {"id" : 70031, "npc_dialogue" : "前几天我在店里买了个包"},
	70032 : {"id" : 70032, "npc_dialogue" : "恩，挺好看的啊"},
	70033 : {"id" : 70033, "npc_dialogue" : "可是今天我朋友也买了这个包，价格居然比我便宜一半"},
	70034 : {"id" : 70034, "npc_dialogue" : "正常啊，双十一打折你不知道？"},
	70041 : {"id" : 70041, "npc_dialogue" : "逛什么商场啊，网购不行吗"},
	70042 : {"id" : 70042, "npc_dialogue" : "我也是这么想的"},
	70043 : {"id" : 70043, "npc_dialogue" : "那你还来逛商场？"},
	70044 : {"id" : 70044, "npc_dialogue" : "先试啊，试完了网上找"},
	70051 : {"id" : 70051, "npc_dialogue" : "芙蓉花又栖满了枝头，奈何蝶难留"},
	70052 : {"id" : 70052, "npc_dialogue" : "还别说，挺有意境的，你写的诗？"},
	70053 : {"id" : 70053, "npc_dialogue" : "刚商场放的歌词"},
	70054 : {"id" : 70054, "npc_dialogue" : "好吧"},
	70061 : {"id" : 70061, "npc_dialogue" : "今天周五，吃饭又得排队"},
	70062 : {"id" : 70062, "npc_dialogue" : "还不是因为你太慢了"},
	70063 : {"id" : 70063, "npc_dialogue" : "怪我咯，不等我也是堵车，等我还省油呢"},
	70064 : {"id" : 70064, "npc_dialogue" : "……合着我还得谢谢你"},
	70071 : {"id" : 70071, "npc_dialogue" : "喜欢一个人就努力去追，去爱，去表白"},
	70072 : {"id" : 70072, "npc_dialogue" : "那要是被拒绝呢？"},
	70073 : {"id" : 70073, "npc_dialogue" : "哪怕被拒绝也不要轻言放弃，经历过十几次失败之后，你就消停了"},
	70074 : {"id" : 70074, "npc_dialogue" : "……"}
}
